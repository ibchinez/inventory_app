      <%= simple_form_for :message, :url => url_for(:action => "create", :controller => "homes") ,remote: true , validate:true do |f| %> 
      <div class="form-group">
          <%= f.input :name , required:true, placeholder:"Jack Ma" %>
        </div>

        <div class="form-group">
          <%= f.input :email,collection: @add_values, required:true, placeholder:"user@example.com" %>
        </div>
        <div class="form-group">
         <%= f.input :mobile_no , as: :tel,  input_html:{ maxlength: 11}, placeholder:"Phone number" %>
        </div>
        <div class="form-group">
          <%= f.input :message , class:"form-control" ,placeholder:"Type your message here" ,  as: :text  , input_html:{ rows: '5' }, required: true%>
        </div>
            <%= f.submit "Send Message", class: "btn btn-primary" %>
      <% end %>            
